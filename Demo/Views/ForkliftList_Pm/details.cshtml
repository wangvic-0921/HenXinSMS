                                <label>预开始时间:</label>
                                <input class="form-control" type="text"id="EstimateTime_Start" value=" @Html.DisplayFor(model => model.EstimateTime_Start)" />                           </dl>  
                                 <dl class="dl-horizontal">
                                <label>预结束时间:</label>
                                <input class="form-control" type="text"id="EstimateTime_End" value=" @Html.DisplayFor(model => model.EstimateTime_End)" />                           </dl>  
